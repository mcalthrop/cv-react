{
  "name": "cv",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "contentful": "7.0.3",
    "muicss": "0.9.41",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-scripts": "1.1.5"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "prettier": "prettier src/**/*.js",
    "prettier:check": "yarn prettier --list-different",
    "prettier:fix": "yarn prettier --write",
    "help": "react-scripts --version",
    "start": "react-scripts start",
    "test": "yarn prettier:check && CI=true react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom"
  },
  "devDependencies": {
    "husky": "1.0.0-rc.14",
    "prettier": "1.14.2"
  }
}
